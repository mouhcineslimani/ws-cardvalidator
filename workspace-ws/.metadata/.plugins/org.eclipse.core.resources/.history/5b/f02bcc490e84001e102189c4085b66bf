package org.mql.ws.restws;

import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.boot.builder.SpringApplicationBuilder;
import org.springframework.boot.web.servlet.support.SpringBootServletInitializer;

@SpringBootApplication
public class PhoneValidatorApplication extends SpringBootServletInitializer {

	public static void main(String[] args) {
	    new PhoneValidatorApplication().configure(new SpringApplicationBuilder(PhoneValidatorApplication.class)).run(args);

		// SpringApplication.run(PhoneValidatorApplication.class, args);
	}

}